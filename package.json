{
  "name": "client-manager",
  "version": "1.0.0",
  "description": "My fabulous Nuxt.js project",
  "author": "noriyuki shimizu <noriyuki.shimizu0612@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "dev:firebase": "npm run generate && firebase serve",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "deploy": "firebase deploy --only hosting:hfaf-client-manager",
    "generate": "nuxt-ts generate",
    "lint": "eslint --fix --ext .ts,.vue --ignore-path .gitignore .",
    "test": "jest",
    "postmerge": "(git diff 'HEAD@{1}' --name-only | grep package.json > /dev/null) && npm ci || :"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{vue,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.3.8",
    "ant-design-vue": "^1.4.10",
    "firebase": "^7.7.0",
    "nuxt": "^2.11.0",
    "nuxt-property-decorator": "2.5.1",
    "vue": "^2.6.11",
    "vue-property-decorator": "^8.3.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.6",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-nuxt": ">=0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "firebase-tools": "^7.12.1",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "vue-jest": "^4.0.0-0"
  },
  "engines": {
    "node": ">= 10.7.0",
    "npm": ">= 6.1.0"
  }
}
